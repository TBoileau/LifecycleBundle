{
  "name": "tboileau/form-handler-bundle",
  "description": "Symfony 4 bundle, a form handler to respect SOLID concepts",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "symfony/framework-bundle": "^4"
  },
  "require-dev": {
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/dotenv": "^4",
    "symfony/maker-bundle": "^1.4",
    "symfony/phpunit-bridge": "^4",
    "symfony/yaml": "^4.2@dev",
    "phpunit/php-code-coverage": "^6.0@dev",
    "symfony/browser-kit": "^4.2@dev",
    "codeclimate/php-test-reporter": "^0.3.0@dev",
    "symfony/console": "^4.2@dev",
    "symfony/twig-bundle": "^4",
    "symfony/security-bundle": "^4"
  },
  "autoload": {
    "psr-4": {
      "TBoileau\\LifecycleBundle\\": "src/",
      "TBoileau\\LifecycleBundle\\Tests\\": "tests/",
      "App\\": "examples/src"
    }
  }
}
